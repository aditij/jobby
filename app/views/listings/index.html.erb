<h1>Listing listings</h1>

<table>
  <tr>
	<th>Date Added</th>
	<th>Position</th>
    <th>Company</th>
    <th>URL</th>
    <th>Priority</th>
    <th>Uid</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @listings.each do |listing| %>
  <tr>
    <td><%= listing.added %></td>
	<td><%= listing.position %></td>
    <td><%= listing.company %></td>
    <td><%= listing.url %></td>
    <td><%= listing.priority %></td>
    <td><%= listing.uid %></td>
    <td><%= link_to 'Show', listing %></td>
    <td><%= link_to 'Edit', edit_listing_path(listing) %></td>
    <td><%= link_to 'Destroy', listing, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Listing', new_listing_path %>
